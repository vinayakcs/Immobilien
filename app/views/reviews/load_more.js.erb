var utils=immobilien_utils();
<% if !@reviews.nil? %>
<% for review in @reviews %>
$('#reviewContainer').append("<%=  escape_javascript render :partial => 'reviews/review', :locals => {:review => review,:usertype => @usertype} %>");
<%end%>
<% end %>
<% if @isLastResults==true %>
$('#reviewContainer').after("<%= escape_javascript render :partial => 'shared/noresult' %>");
$('#load_more_link').remove();
<%else%> 
var oldUrl=$('#load_more_link').attr('href');
$('#load_more_link').attr('href',utils.setLoadMoreParam(oldUrl));
<%end%>
